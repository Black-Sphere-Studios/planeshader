<?xml version="1.0" encoding="UTF-8"?>
<fg:Layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:fg="featherGUI" xsi:schemaLocation="featherGUI feather.xsd" skin="testskin" padding="0,20,0,0">
  <Button name="buttontest" flags="EXPAND">
    <Resource resource="../media/circle.png" color="0xFFFFFFFF" flags="EXPAND|IGNORE" />
  </Button>
  <Window area="130 100 510 420" text="test window" color="0xFFFFFFFF" font="arial.ttf;14" padding="10,22,10,10" flags="RESIZABLE">
    <TabControl area="0 0 100% 100%">
      <Tab name="Tab 1" id="#tabfocus">
        <Button area="10, 10, 0, 0" flags="EXPAND" text="Not Pressed" onaction="makepressed" />
        <Box area="10, 160, 150, 240" flags="TILEY|EXPANDX" max-width="100" margin="2,2,2,2">
          <Checkbox text="Check test 1" flags="EXPAND" margin="2,2,2,2"/>
          <Checkbox text="Check test 22" flags="EXPAND" margin="2,2,2,2"/>
          <Checkbox text="Check test 3" flags="EXPAND" margin="2,2,2,2"/>
          <Checkbox text="Check test 4" flags="EXPAND" margin="2,2,2,2"/>
          <Checkbox text="Check test 5" flags="EXPAND" margin="2,2,2,2"/>
        </Box>
        <Radiobutton area="190, 130, 0, 0" text="Radio Test 1" flags="EXPAND" />
        <Radiobutton area="190, 160, 0, 0" text="Radio Test 2" flags="EXPAND" />
        <Radiobutton area="190, 190, 0, 0" text="Radio Test 3" flags="EXPAND" />
        <Slider area="10, 70, 150, 90" value="200" range="500" onsetvalue="statelistener" />
        <Progressbar id="#progbar" value="0.35" area="10, 100, 150, 125" text="Progress" />
      </Tab>
      <Tab name="Long Tab">
        <Textbox area="140, 30, 210, 150" flags="WORDWRAP" placeholder="placeholder" />
        <Dropdown area="10, 200, 150, 230" flags="TILEY" max-height="60">
          <Text area="0, 0, 100%, 20" text="Drop 1" />
          <Text area="0, 0, 100%, 20" text="Drop 2" />
          <Text flags="EXPAND" text="Drop 3" />
          <Text area="0, 0, 100%, 20" text="Drop 4" />
        </Dropdown>
        <List area="-350:1, 30, -250:1, 0" flags="TILEY|EXPANDY|MULTISELECT|DRAGGABLE" value="3">
          <ListItem area="0, 0, 100%, 20" />
          <Text area="0, 0, 100%, 20" text="List 1" />
          <Text area="0, 0, 100%, 20" text="List 2" />
          <Text area="0, 0, 100%, 20" text="List 3" />
          <Text area="0, 0, 100%, 20" text="List 4" />
          <ListItem area="0, 0, 100%, 20" />
          <Text area="0, 0, 100%, 20" text="List 5" />
        </List>
        <Treeview area="240, 10, 340, 150">
          <TreeItem><Text text="Parent Item 1" flags="EXPAND"/>
            <TreeItem><Text text="Item 4" flags="EXPAND"/></TreeItem>
            <TreeItem><Text text="Item 5" flags="EXPAND"/></TreeItem>
            <TreeItem><Text text="Item 6" flags="EXPAND"/></TreeItem>
          </TreeItem>
          <TreeItem><Text text="Parent Item 2" flags="EXPAND"/>
            <TreeItem><Text text="Item 6" flags="EXPAND"/></TreeItem>
          </TreeItem>
          <TreeItem><Text text="Parent Item 3" flags="EXPAND"/></TreeItem>
        </Treeview>
      </Tab>
      <Tab name="Tab 3">
		<Box area="0,0,100%,0" flags="TILE|EXPANDY" padding="5,5,5,5">
		  <Resource uv="3,3,3,3" area="0,0,100%,100%" color="0xFF999999" flags="ROUNDRECT|BACKGROUND|IGNORE" order="1"/>
        <Box area="0,0,100%,0" flags="TILE|EXPANDY" padding="5,5,5,5" max-width="90" min-width="60">
          <Resource uv="3,3,3,3" area="0,0,100%,100%" color="0xFFBBBB00" flags="ROUNDRECT|BACKGROUND|IGNORE" order="1"/>
          <Resource uv="3,3,3,3" area="0,0,50,20" color="0xFFDDDDDD" flags="ROUNDRECT" margin="0,0,0,0" />
          <Resource uv="3,3,3,3" area="0,0,30,20" color="0xFFDDDDDD" flags="ROUNDRECT" margin="10,0,0,0" /> 
        </Box>
        <Box area="0,0,100%,0" flags="TILE|EXPANDY" padding="5,5,5,5" margin="10,0,0,0" max-width="250">
          <Resource uv="3,3,3,3" area="0,0,100%,100%" color="0xFFBBBBBB" flags="ROUNDRECT|BACKGROUND|IGNORE" order="1"/>
          <Resource uv="3,3,3,3" area="0,0,30,20" color="0xFFDDDDDD" flags="ROUNDRECT" margin="10,0,0,0" />
          <Resource uv="3,3,3,3" area="0,0,100%,20" color="0xFFDDDDDD" flags="ROUNDRECT" margin="10,0,0,0" min-width="60" max-width="100"/> 
          <Resource uv="3,3,3,3" area="0,0,100%,20" color="0xFFDDDDDD" flags="ROUNDRECT" margin="10,0,0,0" min-width="10"/> 
          <Resource uv="3,3,3,3" area="0,0,100%,20" color="0xFFDDDDDD" flags="ROUNDRECT" margin="10,0,0,0" max-width="100"/> 
        </Box>
      </Box>
      </Tab>
      <Tab name="Tab 4">
        <Grid area="0,0,100%,100%" margin="20,20,20,20" padding="0,20,0,0" flags="TILEY|DRAGGABLE" splitter="2,2">
          <Column name="Column 1"/>
          <Column name="Column 2"/>
          <Column name="Column 3"/>
          <Column name="Column 4"/>
          <Column name="Column 5"/>
          <GridRow flags="TILEX|EXPAND">
            <Text text="text 11" flags="EXPANDY"/>
            <Text text="text 12" flags="EXPANDY"/>
            <Text text="text 13" flags="EXPANDY"/>
            <Text text="text 14" flags="EXPANDY"/>
          </GridRow>
          <GridRow flags="TILEX|EXPAND">
            <Text text="text 21" flags="EXPANDY"/>
            <Text text="text 22" flags="EXPANDY"/>
            <Text text="text 23" flags="EXPANDY"/>
            <Text text="text 24" flags="EXPANDY"/>
          </GridRow>
          <GridRow flags="TILEX|EXPAND">
            <Text text="text 31" flags="EXPANDY"/>
            <Text text="text 32" flags="EXPANDY"/>
            <Text text="text 33" flags="EXPANDY"/>
            <Text text="text 34" flags="EXPANDY"/>
          </GridRow>
          <GridRow flags="TILEX|EXPAND" >
            <Text text="text 41" flags="EXPANDY"/>
            <Text text="text 42" flags="EXPANDY"/>
            <Text text="text 43" flags="EXPANDY"/>
            <Text text="text 44" flags="EXPANDY"/>
          </GridRow>
        </Grid>
      </Tab>
    </TabControl>
  </Window>  
  <Menu flags="EXPANDY|TILEX|BACKGROUND">
    <MenuItem text="File">
      <Submenu>
        <MenuItem text="New" />
        <MenuItem text="Open" />
        <MenuItem text="Save" />
        <MenuItem text="Save As..." />
        <MenuItem />
        <MenuItem text="Quit" />
      </Submenu>
    </MenuItem>
    <MenuItem text="Edit">
      <Submenu>
        <MenuItem text="Cut" />
        <MenuItem text="Copy" />
        <MenuItem text="Paste" />
        <MenuItem />
        <MenuItem text="Transform" >
          <Submenu >
            <MenuItem text="Free Transform" >
              <Submenu>
                <MenuItem text="Apply" />
              </Submenu>
            </MenuItem>
            <MenuItem />
            <MenuItem text="Rotate" />
            <MenuItem text="Skew" />
            <MenuItem text="Resize" />
          </Submenu>
        </MenuItem>
      </Submenu>
    </MenuItem>
    <MenuItem text="Options" />
    <MenuItem text="Help">
      <Submenu>
        <MenuItem text="About" />
      </Submenu>
    </MenuItem>
  </Menu>
</fg:Layout>
<fg:Skin name="testskin" xmlns:fg="featherGUI">
  <Skin name="buttontest" color="0xFFFFFFFF" font="arial.ttf;14">
    <Skin name="Resource">
      <Style name="neutral" color="0xFFFF00FF" />
      <Style name="active" color="0xFF00FFFF" />
      <Style name="hover" color="0xFFFFFF00" />
    </Skin>
    <Style name="neutral" text="Neutral" />
    <Style name="active" text="Active" />
    <Style name="hover" text="Hover" />
  </Skin>
  <Skin name="Scrollbar">
    <Skin name="Scrollbar$horzbg" area="0, -20:1, 0:1, 0:1" padding="20,0,20,0">
      <Resource uv="0,0,0,0" area="0,0,100%,100%" color="0x66000000" edgecolor="0" outline="0" flags="ROUNDRECT|BACKGROUND|IGNORE" />
    </Skin>
    <Skin name="Scrollbar$vertbg" area="-20:1, 0, 0:1, 0:1" padding="0,20,0,20">
      <Resource uv="0,0,0,0" area="0,0,100%,100%" color="0x66000000" edgecolor="0" outline="0" flags="ROUNDRECT|BACKGROUND|IGNORE" />
    </Skin>
    <Skin name="Scrollbar$corner">
      <Resource uv="0,0,0,0" area="0,0,100%,100%" color="0x66000000" edgecolor="0" outline="0" flags="ROUNDRECT|BACKGROUND|IGNORE" />
    </Skin>
    <Skin name="Scrollbar$scrollhorz">
      <Resource name="#scrollbg" uv="6,6,6,6" area="0,0,100%,100%" color="0x660000FF" edgecolor="0xFF0000FF" outline="1" flags="ROUNDRECT|BACKGROUND|IGNORE" />
      <Skin name="#scrollbg">
        <Style name="disabled" edgecolor="0xFF999999"/>
        <Style name="neutral" edgecolor="0xFF0000FF"/>
      </Skin>
    </Skin>
    <Skin name="Scrollbar$scrollvert">
      <Resource name="#scrollbg" uv="6,6,6,6" area="0,0,100%,100%" color="0x660000FF" edgecolor="0xFF0000FF" outline="1" flags="ROUNDRECT|BACKGROUND|IGNORE" />
      <Skin name="#scrollbg">
        <Style name="disabled" edgecolor="0xFF999999"/>
        <Style name="neutral" edgecolor="0xFF0000FF"/>
      </Skin>
    </Skin>
    <Skin name="Scrollbar$scrollleft" area="0,0,20,20">
      <Resource name="#scrollbg" uv="6,6,6,6" area="0,0,100%,100%" color="0" edgecolor="0xFF0000FF" outline="1" flags="ROUNDRECT|BACKGROUND" />
      <Skin name="#scrollbg">
        <Style name="disabled" edgecolor="0xFF999999"/>
        <Style name="neutral" edgecolor="0xFF0000FF"/>
      </Skin>
    </Skin>
    <Skin name="Scrollbar$scrolltop" area="0,0,20,20">
      <Resource name="#scrollbg" uv="6,6,6,6" area="0,0,100%,100%" color="0" edgecolor="0xFF0000FF" outline="1" flags="ROUNDRECT|BACKGROUND" />
      <Skin name="#scrollbg">
        <Style name="disabled" edgecolor="0xFF999999"/>
        <Style name="neutral" edgecolor="0xFF0000FF"/>
      </Skin>
    </Skin>
    <Skin name="Scrollbar$scrollright" area="-20:1, 0, 0:1, 20">
      <Resource name="#scrollbg" uv="6,6,6,6" area="0,0,100%,100%" color="0" edgecolor="0xFF0000FF" outline="1" flags="ROUNDRECT|BACKGROUND" />
      <Skin name="#scrollbg">
        <Style name="disabled" edgecolor="0xFF999999"/>
        <Style name="neutral" edgecolor="0xFF0000FF"/>
      </Skin>
    </Skin>
    <Skin name="Scrollbar$scrollbottom" area="0, -20:1, 20, 0:1">
      <Resource name="#scrollbg" uv="6,6,6,6" area="0,0,100%,100%" color="0" edgecolor="0xFF0000FF" outline="1" flags="ROUNDRECT|BACKGROUND" />
      <Skin name="#scrollbg">
        <Style name="disabled" edgecolor="0xFF999999"/>
        <Style name="neutral" edgecolor="0xFF0000FF"/>
      </Skin>
    </Skin>
  </Skin>
  <Skin name="Window" flags="RESIZABLE">
    <Resource uv="5,5,5,5" area="0,0,100%,100%" color="0xFF666666" edgecolor="0xFFAAAAAA" outline="1" flags="ROUNDRECT|BACKGROUND" />
    <Skin name="Window$text" margin="4,4,0,0" />
    <Skin name="Window$restore" area="-30:1, 0, -15:1, 15" >
      <Resource area="0,0,100%,100%" color="0" edgecolor="0xFFDDDDDD" outline="3" flags="ROUNDRECT|BACKGROUND" />
    </Skin>
    <Skin name="Button" color="0xFFFFFFFF" font="arial.ttf;14" padding="5,5,5,5">
      <Skin name="#bg">
        <Style name="neutral" color="0xFF666666" />
        <Style name="active" color="0xFF505050" />
        <Style name="hover" color="0xFF7F7F7F" />
      </Skin>
      <Resource name="#bg" uv="5,5,5,5" area="0,0,100%,100%" color="0xFF666666" edgecolor="0xFFAAAAAA" outline="1" flags="ROUNDRECT|BACKGROUND" />
    </Skin>
    <Skin name="Checkbox" color="0xFFFFFFFF" font="arial.ttf;14" padding="25,0,5,0">
      <Resource name="#checkbg" uv="3,3,3,3" area="5, 0:0.5, 20, 15:0.5" color="0xFFFFFFFF" edgecolor="0xFF222222" outline="1" flags="ROUNDRECT|BACKGROUND|SNAP" center="0 50%" />
      <Resource name="#check" uv="3,3,3,3" area="8, 0:0.5, 17, 9:0.5" color="0xFF000000" edgecolor="0" outline="0" flags="ROUNDRECT|BACKGROUND|SNAP" center="0 50%" order="1" />
      <Skin name="#checkbg">
        <Style name="neutral" color="0xFFFFFFFF" />
        <Style name="active" color="0xFFBBBBBB" />
        <Style name="hover" color="0xFFDDDDDD" />
      </Skin>
      <Skin name="#check">
        <Style name="default" flags="HIDDEN" />
        <Style name="checked" flags="-HIDDEN" />
        <Style name="indeterminate" flags="HIDDEN" />
      </Skin>
    </Skin>
    <Skin name="Radiobutton" color="0xFFFFFFFF" font="arial.ttf;14" padding="25,5,5,5">
      <Resource name="#bg" uv="0,6.2832,0,6.2832" area="5, 0:0.5, 20, 15:0.5" color="0xFFFFFFFF" edgecolor="0xFF222222" outline="1" flags="CIRCLE|BACKGROUND|SNAP" center="0 50%" />
      <Resource name="#check" uv="0,6.2832,0,6.2832" area="8, 0:0.5, 17, 9:0.5" color="0xFF000000" edgecolor="0" outline="0" flags="CIRCLE|BACKGROUND|SNAP" center="0 50%" order="1"/>
      <Skin name="#bg">
        <Style name="neutral" color="0xFFFFFFFF" />
        <Style name="active" color="0xFFBBBBBB" />
        <Style name="hover" color="0xFFDDDDDD" />
      </Skin>
      <Skin name="#check">
        <Style name="default" flags="HIDDEN" />
        <Style name="checked" flags="-HIDDEN" />
      </Skin>
    </Skin>
    <Skin name="Progressbar" color="0xFFFFFFFF" font="arial.ttf;14" padding="0,0,0,0">
      <Resource uv="6,6,6,6" area="0,0,100%,100%" color="0" edgecolor="0xFFDDDDDD" outline="3" flags="ROUNDRECT|BACKGROUND" />
      <Skin name="Progressbar$bar">
        <Resource uv="6,6,6,6" area="0,0,100%,100%" color="0xFF333333" edgecolor="0" outline="0" flags="ROUNDRECT" />
      </Skin>
    </Skin>
    <Skin name="Slider" padding="5,0,5,0">
      <Curve area="0, 50%, 100%, 1.0:0.5" flags="LINE" color="0xFFFFFFFF" />
      <Skin name="Slider$slider">
        <Resource uv="5,5,5,5" area="0,0,10,20" color="0xFFFFFFFF" edgecolor="0xFF666666" outline="1" flags="ROUNDRECT|NOCLIP" />
      </Skin>
    </Skin>
    <Skin name="Box" inherit="Scrollbar" />
    <Skin name="Textbox" color="0xFFFFFFFF" font="arial.ttf;14" lineheight="16" placecolor="0x88FFFFFF" cursorcolor="0xFFFFFFFF" selectcolor="0xAAAA0000" padding="3,3,3,3" inherit="Scrollbar">
      <Resource name="#textbg" uv="3,3,3,3" area="0,0,100%,100%" color="0x99000000" edgecolor="0xFFAAAAAA" outline="1" flags="ROUNDRECT|BACKGROUND|IGNORE" />
    </Skin>
    <Skin name="Dropdown" hovercolor="0x99990099" selectcolor="0xAAAA0000">
      <Resource name="#dropdownbg" uv="3,3,3,3" area="0,0,100%,100%" color="0x99000000" edgecolor="0xFFAAAAAA" outline="1" flags="ROUNDRECT|BACKGROUND|IGNORE" />
      <Skin name="Dropdown$box" inherit="Scrollbar">
        <Resource name="#dropdownboxbg" uv="3,3,3,3" area="0,0,100%,100%" color="0x99000000" edgecolor="0xFFAAAAAA" outline="1" flags="ROUNDRECT|BACKGROUND|IGNORE" />
      </Skin>
    </Skin>
  </Skin>
  <Skin id="Treeview">
    <Resource uv="3,3,3,3" area="0,0,100%,100%" color="0x99000000" edgecolor="0xFFAAAAAA" outline="1" flags="ROUNDRECT|BACKGROUND|IGNORE" />
    <Skin name="Text" color="0xFFFFFFFF" font="arial.ttf;10" />
    <Skin name="TreeItem" padding="20,0,0,0" flags="EXPAND">
      <Resource name="#bg" uv="2,2,2,2" area="0,0,100%,100%" color="0x99AA6666" edgecolor="0xAAFFAAAA" outline="1" flags="ROUNDRECT|BACKGROUND|IGNORE" />
      <Skin name="TreeItem$arrow" area="4,1,16,13">
        <Resource name="#bg" uv="3,3,3,3" area="0,0,100%,100%" color="0" edgecolor="0xFFDDDDDD" outline="1" flags="ROUNDRECT|BACKGROUND|IGNORE" />
        <Resource name="#bgv" uv="0,0,0,0" area="-1:0.5, 2, 1:0.5, -2:1" color="0xFFDDDDDD" flags="ROUNDRECT|BACKGROUND|IGNORE" />
        <Resource name="#bgh" uv="0,0,0,0" area="2, -1:0.5, -2:1, 1:0.5" color="0xFFDDDDDD" flags="ROUNDRECT|BACKGROUND|IGNORE" />
        <Skin name="#bgv">
          <Style name="hidden" flags="-HIDDEN"/>
          <Style name="visible" flags="HIDDEN"/>
        </Skin>
      </Skin>
      <Skin name="#bg">
        <Style name="neutral+focused" flags="-HIDDEN"/>
        <Style name="neutral" flags="HIDDEN"/>
      </Skin>
    </Skin>
  </Skin>
  <Skin name="TabControl">
    <Skin name="Tabcontrol$toggle" color="0xFFFFFFFF" margin="0,0,10,0" padding="6,2,6,2">
      <Resource name="#tabtogglebg" uv="3,3,3,3" area="0,0,100%,100%" color="0x99000000" edgecolor="0xFFAAAAAA" outline="1" flags="ROUNDRECT|BACKGROUND" />
      <Style name="default" edgecolor="0xBBFFFFFF"/>
      <Style name="checked" edgecolor="0xFFFFFFFF"/>
    </Skin>
  </Skin>
  <Skin name="List">
    <Resource name="#listbg" uv="3,3,3,3" area="0,0,100%,100%" color="0x99000000" edgecolor="0xFFAAAAAA" outline="1" flags="ROUNDRECT|BACKGROUND" />
  </Skin>
  <Skin name="ListItem">
    <Resource name="#listbg" uv="3,3,3,3" area="0,0,100%,100%" color="0x99000000" edgecolor="0xFFAAAAAA" outline="1" flags="ROUNDRECT|BACKGROUND" />
    <Skin name="#listbg">
      <Style name="hover" color="0x99009900" />
      <Style name="neutral" color="0x99990099" />
      <Style name="neutral+selected" color="0x99999900" />
      <Style name="hover+selected" color="0xCC999900" />
    </Skin>
  </Skin>
  <Skin name="Grid">
    <Resource name="#gridbg" uv="3,3,3,3" area="0,0,100%,100%" color="0x99000000" edgecolor="0xFFAAAAAA" outline="1" flags="ROUNDRECT|BACKGROUND" />
  </Skin>
  <Skin name="Text" color="0xFFFFFFFF" font="arial.ttf;14" />
  <Skin name="Menu" hovercolor="0xFFFFFFFF">
    <Resource name="#menubg" uv="0,0,0,0" area="0,0,100%,100%" color="0x99000000" edgecolor="0xFFAAAAAA" outline="0" flags="ROUNDRECT|BACKGROUND" />
  </Skin>
  <Skin name="Submenu">
    <Resource name="#submenubg" uv="0,3,3,0" area="0,0,100%,100%" color="0x99000000" edgecolor="0xFFAAAAAA" outline="1" flags="ROUNDRECT|BACKGROUND" />
    <Skin name="Submenu$seperator" area="0,0,100%,5">
      <Resource area="0,0,100%,100%" color="0xDDCCCCCC" outline="0" flags="ROUNDRECT|BACKGROUND" margin="0,2,0,2" />
    </Skin>
    <Skin name="MenuItem" padding="0,0,20,0" />
    <Skin name="Submenu$arrow" area="100%, 50%, 100%, 50%" center="100%,50%">
      <Resource area="0,0,10,10" color="0xDDCCCCCC" outline="0" flags="ROUNDRECT|IGNORE" />
    </Skin>
  </Skin>
  <Skin name="Debug" color="0xFFFFFFFF" font="arial.ttf;14" />
</fg:Skin>